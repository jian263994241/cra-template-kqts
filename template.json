{
  "package": {
    "dependencies": {
      "@types/node": "^17.0.42",
      "@types/react": "^18.0.12",
      "@types/react-dom": "^18.0.5",
      "axios": "^0.27.2",
      "clsx": "^1.2.1",
      "querystringify": "^2.2.0",
      "react-router-dom": "^6.4.2"
    },
    "devDependencies": {
      "@commitlint/cli": "^17.0.2",
      "@commitlint/config-conventional": "^17.0.2",
      "@testing-library/jest-dom": "^5.16.4",
      "@testing-library/react": "^13.3.0",
      "@testing-library/user-event": "^14.3.0",
      "commitizen": "^4.2.4",
      "cz-conventional-changelog": "^3.3.0",
      "env-cmd": "^10.1.0",
      "eslint-config-prettier": "^8.5.0",
      "eslint-plugin-prettier": "^4.2.1",
      "filemanager-webpack-plugin": "^7.0.0",
      "glob": "^8.0.3",
      "http-proxy-middleware": "^2.0.6",
      "husky": "^8.0.1",
      "mocker-api": "^2.9.5",
      "prettier": "^2.6.2",
      "pretty-quick": "^3.1.3",
      "react-app-rewire-multiple-entry": "^2.2.2",
      "react-app-rewired": "^2.2.1",
      "sass": "^1.54.4",
      "typescript": "^4.7.3",
      "web-vitals": "^2.1.4",
      "webpackbar": "^5.0.2"
    },
    "scripts": {
      "start": "env-cmd -e development react-app-rewired start",
      "start:mock": "env-cmd -e mock npm run start",
      "build": "env-cmd -e production react-app-rewired build",
      "postbuild": "react-snap",
      "test": "react-scripts test",
      "commit": "cz",
      "zip": "rm -rf zip || true && mkdir zip && cd ./build && zip -r ../zip/build.zip ./"
    },
    "eslintConfig": {
      "extends": [
        "react-app",
        "prettier",
        "plugin:prettier/recommended"
      ]
    },
    "reactSnap": {
      "source": "build",
      "inlineCss": false,
      "publicPath": "/",
      "minifyHtml": {
        "collapseWhitespace": false,
        "removeComments": false,
        "sortClassName": false
      }
    },
    "browserslist": {
      "production": [
        "last 1 version",
        "> 1%",
        "IE 11"
      ],
      "development": [
        "last 1 chrome version",
        "last 1 firefox version",
        "last 1 safari version"
      ]
    },
    "husky": {
      "hooks": {
        "pre-commit": "pretty-quick --staged --pattern \"**/*.*(js|jsx)\"",
        "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
      }
    },
    "config": {
      "commitizen": {
        "path": "./node_modules/cz-conventional-changelog"
      }
    }
  }
}