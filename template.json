{
  "package": {
    "dependencies": {
      "@types/node": "^20.8.7",
      "@types/react": "^18.2.29",
      "@types/react-dom": "^18.2.14",
      "ahooks": "^3.7.8",
      "axios": "^1.6.1",
      "cache2": "^1.1.1",
      "clsx": "^2.0.0",
      "querystringify": "^2.2.0",
      "react": "^18.2.0",
      "react-app-polyfill": "^3.0.0",
      "react-dom": "^18.2.0",
      "react-helmet-async": "^1.3.0",
      "react-router-dom": "^6.17.0",      
      "util-helpers": "^4.20.8"
    },
    "devDependencies": {
      "@commitlint/cli": "^17.8.0",
      "@commitlint/config-conventional": "^17.8.0",
      "@testing-library/jest-dom": "^6.1.4",
      "@testing-library/react": "^14.0.0",
      "@testing-library/user-event": "^14.5.1",
      "commitizen": "^4.3.0",
      "cz-conventional-changelog": "^3.3.0",
      "env-cmd": "^10.1.0",
      "eslint-config-prettier": "^9.0.0",
      "eslint-plugin-prettier": "^5.0.1",
      "filemanager-webpack-plugin": "^8.0.0",
      "glob": "^10.3.10",
      "http-proxy-middleware": "^2.0.6",
      "husky": "^8.0.1",
      "jest": "^29.7.0",
      "mocker-api": "^2.9.8",
      "prettier": "^3.0.3",
      "pretty-quick": "^3.1.3",
      "react-app-rewire-multiple-entry": "^2.2.3",
      "react-app-rewired": "^2.2.1",
      "react-scripts": "5.0.1",
      "react-snap": "^1.23.0",
      "sass": "^1.69.4",
      "typescript": "^5.2.2",
      "web-vitals": "^3.5.0",
      "webpackbar": "^5.0.2"
    },
    "scripts": {
      "start": "env-cmd -e development react-app-rewired start",
      "start:mock": "env-cmd -e mock npm run start",
      "build": "env-cmd -e production react-app-rewired build",
      "postbuild": "react-snap",
      "test": "react-scripts test",
      "commit": "cz",
      "zip": "rm -rf zip || true && mkdir zip && cd ./build && zip -r ../zip/build.zip ./"
    },
    "eslintConfig": {
      "extends": [
        "react-app",
        "prettier",
        "plugin:prettier/recommended"
      ]
    },
    "reactSnap": {
      "source": "build",
      "inlineCss": false,
      "publicPath": "/",
      "skipThirdPartyRequests": true,
      "include": ["/"],
      "minifyHtml": {
        "collapseWhitespace": false,
        "removeComments": false,
        "sortClassName": false
      }
    },
    "browserslist": {
      "production": [
        "last 1 version",
        "> 1%",
        "IE 11"
      ],
      "development": [
        "last 1 chrome version",
        "last 1 firefox version",
        "last 1 safari version"
      ]
    },
    "husky": {
      "hooks": {
        "pre-commit": "pretty-quick --staged --pattern \"**/*.*(js|jsx)\"",
        "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
      }
    },
    "config": {
      "commitizen": {
        "path": "./node_modules/cz-conventional-changelog"
      }
    }
  }
}
